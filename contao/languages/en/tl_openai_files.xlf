            <trans-unit id="tl_openai_files.header">
                <source>OpenAI Files</source>
                <target>OpenAI Files</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.intro">
                <source>Here you can manage your files for OpenAI. Upload documents that will be used to train your assistants. Supported formats: PDF, TXT, MD, DOCX, PPTX, JSON.</source>
                <target>Here you can manage your files for OpenAI. Upload documents that will be used to train your assistants. Supported formats: PDF, TXT, MD, DOCX, PPTX, JSON.</target>
            </trans-unit>
            <!-- Legend Labels -->
            <trans-unit id="tl_openai_files.file_legend">
                <source>File Upload</source>
                <target>File Upload</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.openai_legend">
                <source>OpenAI Information</source>
                <target>OpenAI Information</target>
            </trans-unit>
            
            <!-- Field Labels -->
            <trans-unit id="tl_openai_files.filename.0">
                <source>Filename</source>
                <target>Filename</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.filename.1">
                <source>The name of the uploaded file.</source>
                <target>The name of the uploaded file.</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.file_upload.0">
                <source>Select Files</source>
                <target>Select Files</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.file_upload.1">
                <source>Select one or more files to upload in the OpenAI vector store (.pdf, .txt, .md, .docx, .pptx, .json).</source>
                <target>Select one or more files to upload in the OpenAI vector store (.pdf, .txt, .md, .docx, .pptx, .json).</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.openai_file_id.0">
                <source>OpenAI File ID</source>
                <target>OpenAI File ID</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.openai_file_id.1">
                <source>The OpenAI file ID after successful upload.</source>
                <target>The OpenAI file ID after successful upload.</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.status.0">
                <source>Status</source>
                <target>Status</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.status.1">
                <source>Current processing status of the file.</source>
                <target>Current processing status of the file.</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.file_size.0">
