                <source>Pending</source>
                <target>Pending</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.status_options.processing">
                <source>Processing</source>
                <target>Processing</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.status_options.uploaded">
                <source>Uploaded</source>
                <target>Uploaded</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.status_options.completed">
                <source>Completed</source>
                <target>Completed</target>
            </trans-unit>
