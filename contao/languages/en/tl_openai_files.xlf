                <source>File upload</source>
                <target>File upload</target>
            </trans-unit>
            <trans-unit id="tl_openai_files.welcome_message1">
                <source>Welcome to the file upload screen.</source>
                <target>Welcome to the file upload screen.</target>
            </trans-unit>
